<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.geekduxu.xmanager"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="cn.geekduxu.xmanager.SplashActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="cn.geekduxu.xmanager.HomeActivity"
            android:label="@string/title_activity_home" >
        </activity>
        <activity android:name="cn.geekduxu.xmanager.SettingActivity" >
        </activity>
        <activity android:name="cn.geekduxu.xmanager.LostFoundActivity" >
        </activity>
        <activity android:name="cn.geekduxu.xmanager.Setup1Activity" >
        </activity>
        <activity android:name="cn.geekduxu.xmanager.Setup2Activity" >
        </activity>
        <activity android:name="cn.geekduxu.xmanager.Setup3Activity" >
        </activity>
        <activity android:name="cn.geekduxu.xmanager.Setup4Activity" >
        </activity>
    </application>

</manifest>